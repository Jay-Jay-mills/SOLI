{
  "name": "soli-backend",
  "version": "1.0.0",
  "description": "SOLI Enterprise Portal Backend API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "dev:qa": "set NODE_ENV=qa&& nodemon index.js",
    "start:qa": "set NODE_ENV=qa&& node index.js",
    "seed:qa": "set NODE_ENV=qa&& node seeders/createAdmin.js",
    "reset:qa": "node utils/resetQADB.js",
    "dev:test": "set NODE_ENV=test&& nodemon index.js",
    "start:test": "set NODE_ENV=test&& node index.js",
    "seed": "node seeders/createAdmin.js",
    "seed:test": "set NODE_ENV=test&& node seeders/createAdmin.js",
    "reset:test": "node utils/resetTestDB.js",
    "test": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --runInBand",
    "test:coverage": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  }
}
